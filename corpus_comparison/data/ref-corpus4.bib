@article{ ISI:000379415500016,
Author = {Faillettaz, Jerome and Funk, Martin and Vagliasindi, Marco},
Title = {{Time forecast of a break-off event from a hanging glacier}},
Journal = {{CRYOSPHERE}},
Year = {{2016}},
Volume = {{10}},
Number = {{3}},
Pages = {{1191-1200}},
Abstract = {{A cold hanging glacier located on the south face of the Grandes Jorasses
   (Mont Blanc, Italy) broke off on the 23 and 29 September 2014 with a
   total estimated ice volume of 105aEuro-000aEuro-m(3). Thanks to accurate
   surface displacement measurements taken up to the final break-off, this
   event was successfully predicted 10 days in advance, enabling local
   authorities to take the necessary safety measures. The break-off event
   also confirmed that surface displacements experienced a power law
   acceleration along with superimposed log-periodic oscillations prior to
   the final rupture. This paper describes the methods used to achieve a
   satisfactory time forecast in real time and demonstrates, using a
   retrospective analysis, their potential for the development of
   early-warning systems in real time.}},

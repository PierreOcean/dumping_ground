@article{ ISI:000371481100014,
Author = {Mathew, Anugraha and Jenul, Christian and Carlier, Aurelien L. and
   Eberl, Leo},
Title = {{The role of siderophores in metal homeostasis of members of the genus
   Burkholderia}},
Journal = {{ENVIRONMENTAL MICROBIOLOGY REPORTS}},
Year = {{2016}},
Volume = {{8}},
Number = {{1}},
Pages = {{103-109}},
Month = {{FEB}},
Abstract = {{Although members of the genus Burkholderia can utilize a high-affinity
   iron uptake system to sustain growth under iron-limiting conditions,
   many strains also produce siderophores, suggesting that they may serve
   alternative functions. Here we demonstrate that the two Burkholderia
   siderophores pyochelin and ornibactin can protect the cells from metal
   toxicity and thus play an alternative role in metal homeostasis. We also
   demonstrate that metals such as copper and zinc induce the production of
   ornibactin.}},

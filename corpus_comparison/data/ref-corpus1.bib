@article{ ISI:000368561100010,
Author = {van der Heijden, Marcel G. A. and de Bruin, Susanne and Luckerhoff, Ludo
   and van Logtestijn, Richard S. P. and Schlaeppi, Klaus},
Title = {{A widespread plant-fungal-bacterial symbiosis promotes plant
   biodiversity, plant nutrition and seedling recruitment}},
Journal = {{ISME JOURNAL}},
Year = {{2016}},
Volume = {{10}},
Number = {{2}},
Pages = {{389-399}},
Month = {{FEB}},
Abstract = {{Highly diverse microbial assemblages colonize plant roots. It is still
   poorly understood whether different members of this root microbiome act
   synergistically by supplying different services (for example, different
   limiting nutrients) to plants and plant communities. In order to test
   this, we manipulated the presence of two widespread plant root
   symbionts, arbuscular mycorrhizal fungi and nitrogen-fixing rhizobia
   bacteria in model grassland communities established in axenic
   microcosms. Here, we demonstrate that both symbionts complement each
   other resulting in increased plant diversity, enhanced seedling
   recruitment and improved nutrient acquisition compared with a single
   symbiont situation. Legume seedlings obtained up to 15-fold higher
   productivity if they formed an association with both symbionts, opposed
   to productivity they reached with only one symbiont. Our results reveal
   the importance of functional diversity of symbionts and demonstrate that
   different members of the root microbiome can complement each other in
   acquiring different limiting nutrients and in driving important
   ecosystem functions.}},

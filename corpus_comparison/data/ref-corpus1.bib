@article{ ISI:000376807700007,
Author = {Bender, S. Franz and Wagg, Cameron and van der Heijden, Marcel G. A.},
Title = {{An Underground Revolution: Biodiversity and Soil Ecological Engineering
   for Agricultural Sustainability}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{2016}},
Volume = {{31}},
Number = {{6}},
Pages = {{440-452}},
Month = {{JUN}},
Abstract = {{Soil organisms are an integral component of ecosystems, but their
   activities receive little recognition in agricultural management
   strategies. Here we synthesize the potential of soil organisms to
   enhance ecosystem service delivery and demonstrate that soil
   biodiversity promotes multiple ecosystem functions simultaneously (i.e.,
   ecosystem multifunctionality). We apply the concept of ecological
   intensification to soils and we develop strategies for targeted
   exploitation of soil biological traits. We compile promising approaches
   to enhance agricultural sustainability through the promotion of soil

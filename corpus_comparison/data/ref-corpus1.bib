@article{ ISI:000313722400002,
Author = {Seidl, Roman and Brand, Fridolin Simon and Stauffacher, Michael and
   Kruetli, Pius and Le, Quang Bao and Spoerri, Andy and Meylan, Gregoire
   and Moser, Corinne and Gonzalez, Monica Berger and Scholz, Roland Werner},
Title = {{Science with Society in the Anthropocene}},

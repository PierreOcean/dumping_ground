@article{ ISI:000427333500006,
Author = {Thanh Ha Thi Do and Martinoia, Enrico and Lee, Youngsook},
Title = {{Functions of ABC transporters in plant growth and development}},
Journal = {{CURRENT OPINION IN PLANT BIOLOGY}},
Year = {{2018}},
Volume = {{41}},
Pages = {{32-38}},
Month = {{FEB}},
Abstract = {{ABC transporters are essential for plant development, playing roles in
   processes such as gametogenesis, seed development, seed germination,
   organ formation, and secondary growth. ABC transporters are directly
   energized by ATP and can transport complex organic materials against
   concentration gradients; thus, they are uniquely suited to provide the
   complex building blocks required for the development of specialized
   plant cells. We review recent progress in our understanding of the
   contribution ABC transporters make to the growth and development of
   plants, including their roles in protective layer formation and in
   transporting phytohormones.}},

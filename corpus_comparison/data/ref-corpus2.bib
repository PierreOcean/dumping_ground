@article{ ISI:000419242100005,
Author = {Gounand, Isabelle and Harvey, Eric and Little, Chelsea J. and Altermatt,
   Florian},
Title = {{Meta-Ecosystems 2.0: Rooting the Theory into the Field}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{2018}},
Volume = {{33}},
Number = {{1}},
Pages = {{36-46}},
Month = {{JAN}},
Abstract = {{The meta-ecosystem framework demonstrates the significance of
   among-ecosystem spatial flows for ecosystem dynamics and has fostered a
   rich body of theory. The high level of abstraction of the models,
   however, impedes applications to empirical systems. We argue that
   further understanding of spatial dynamics in natural systems strongly
   depends on dense exchanges between field and theory. From empiricists,
   more and specific quantifications of spatial flows are needed, defined
   by the major categories of organismal movement (dispersal, foraging,
   life-cycle, and migration). In parallel, the theoretical framework must
   account for the distinct spatial scales at which these naturally common
   spatial flows occur. Integrating all levels of spatial connections among
   landscape elements will upgrade and unify landscape and meta-ecosystem
   ecology into a single framework for spatial ecology.}},

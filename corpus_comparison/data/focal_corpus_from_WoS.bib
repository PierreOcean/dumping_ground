@article{ ISI:000356491400004,
Author = {Maestrini, Bernardo and Nannipieri, Paolo and Abiven, Samuel},
Title = {{A meta-analysis on pyrogenic organic matter induced priming effect}},
Journal = {{GLOBAL CHANGE BIOLOGY BIOENERGY}},

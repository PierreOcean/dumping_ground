@article{ ISI:000350642000009,
Author = {Geinitz, Steven and Furrer, Reinhard and Sain, Stephan R.},
Title = {{Bayesian multilevel analysis of variance for relative comparison across
   sources of global climate model variability}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},

@article{ ISI:000423508700004,
Author = {Srivastava, Kavita and Jentsch, Anke and Kreyling, Juergen and Glaser,
   Bruno and Wiesenberg, Guido L. B.},
Title = {{Short-term carbon dynamics in a temperate grassland and heathland
   ecosystem exposed to 104 days of drought followed by irrigation}},
Journal = {{ISOTOPES IN ENVIRONMENTAL AND HEALTH STUDIES}},
Year = {{2018}},
Volume = {{54}},
Number = {{1}},
Pages = {{41-62}},
Abstract = {{Temperate ecosystems are susceptible to drought events. The effect of a
   severe drought (104 days) followed by irrigation on the plant C uptake,
   its assimilation and input of C in soil were examined using a triple
   (CO2)-C-13 pulse-chase labelling experiment in model grassland and
   heathland ecosystems. First (CO2)-C-13 pulse at day 0 of the experiment
   revealed much higher C-13 tracer uptake for shoots, roots and soil
   compared to the second pulse (day 44), where all plants showed
   significantly lower C-13 tracer uptake. After the third (CO2)-C-13 pulse
   (day 70), very low C-13 uptake in shoots led to a negligible allocation
   of C-13 into roots and soil. During irrigation after the severe drought,
   the C-13 tracer that was allocated in plant tissues during the second
   and third pulse labelling was re-allocated in roots and soil, as soon as
   the irrigation started. This re-allocation was higher and longer lasting
   in heathland compared to grassland ecosystems.}},

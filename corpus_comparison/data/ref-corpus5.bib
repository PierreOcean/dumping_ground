Author = {Reinhard, Juergen and Zah, Rainer and Hilty, Lorenz M.},
Editor = {{Wohlgemuth, V and FuchsKittowski, F and Wittmann, J}},
Title = {{Regionalized LCI Modeling: A Framework for the Integration of Spatial
   Data in Life Cycle Assessment}},
Booktitle = {{ADVANCES AND NEW TRENDS IN ENVIRONMENTAL INFORMATICS: STABILITY,
   CONTINUITY, INNOVATION}},

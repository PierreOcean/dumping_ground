@inproceedings{ ISI:000388114605019,
Author = {Li, Chengxiu and Wulf, Hendrik and Garonna, Irene and Schaepman, Michael},
Book-Group-Author = {{IEEE}},
Title = {{MONITORING FUNCTIONAL TRAITS OF ALPINE VEGETATION ON THE QINGHAI-TIBET
   PLATEAU USING MULTI-SENSOR REMOTE SENSING}},
Booktitle = {{2016 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM (IGARSS)}},
Series = {{IEEE International Symposium on Geoscience and Remote Sensing IGARSS}},

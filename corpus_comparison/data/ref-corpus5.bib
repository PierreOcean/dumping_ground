@inproceedings{ ISI:000382326300042,
Author = {Ostermann, F. O. and Huang, H. and Andrienko, G. and Andrienko, N. and
   Capineri, C. and Farkas, K. and Purves, R. S.},
Editor = {{Mallet, C and Paparoditis, N and Dowman, I and Elberink, SO and Raimond, AM and Rottensteiner, F and Yang, M and Christophe, S and Coltekin, A and Bredif, M}},
Title = {{EXTRACTING AND COMPARING PLACES USING GEO-SOCIAL MEDIA}},
Booktitle = {{ISPRS GEOSPATIAL WEEK 2015}},
